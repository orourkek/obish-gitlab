{
  "name": "obish-gitlab",
  "version": "0.1.0",
  "description": "",
  "author": "Kevin O'Rourke <kevin@korourke.net>",

  "devDependencies": {
    "less": "latest",
    "less-plugin-clean-css": "latest",
    "chokidar-cli": "latest",
    "twitter-bootstrap-3.0.0": "3.0.0",
    "nprogress": "latest"
  },

  "scripts": {
    "build-css": "lessc --source-map assets/less/obish-gitlab.less assets/css/obish-gitlab.css",
    "watch-css": "npm run build-css; chokidar assets/less/gitlab/*.less assets/less/gitlab/**/*.less assets/less/*.less -c 'npm run build-css'"
  }
}
